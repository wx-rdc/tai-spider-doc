{
    "title": "Tai Spider Reference",
    "author": "Edward Zhu (zhuhy163@163.com)",
    "description": "TaiSpider In Action",
    "language": "en",
    "gitbook": "3.2.3",
    "output.name": "site",
    "plugins": [
        "-lunr",
        "-search",
        "-sharing",
        "-fontsettings",
        "back-to-top-button",
        "cuav-chapters",
        "heading-anchors",
        "search-pro",
        "theme-hqbook"
    ],
    "variables": {
        "themeHqbook": {
            "nav": [
                {
                    "url": "http://...",
                    "target": "_blank",
                    "name": "简易教程"
                }
            ]
        }
    },
    "pluginsConfig": {
        "theme-hqbook": {
            "search-placeholder": "输入关键字搜索",
            "logo": "/logo.jpg",
            "favicon": "/favicon.ico",
            "hide-elements": [
                ".summary .gitbook-link",
                ".navigation"
            ]
        }
    }
}